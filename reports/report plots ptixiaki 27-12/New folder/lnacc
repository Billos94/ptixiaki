n10 = normc(up10);

figure;
hold on
plot(n1(:,1));
plot(n2(:,1));
plot(n3(:,1));
plot(n4(:,1));
plot(n5(:,1));
plot(n7(:,1));
plot(n8(:,1));
plot(n9(:,1));
plot(n10(:,1));
title('accellerometer X axes');
hold off
axis([0 300 -0.2 0.2])

